{
    "clock": {
        "interval": 60,
        "format": "{:%H:%M}",
        "format-alt": "{:%A, %B %d, %Y (%H:%M)}"
    },
    "weather": {
        "interval": 300,
        "format": " {temperature}°",
        "location": "auto:ip"
    },
    "cpu": {
        "interval": 5
    },
    "memory": {
        "interval": 5
    },
    "temperature": {
        "interval": 5,
        "hwmon-path": "/sys/class/hwmon/hwmon0/temp1_input"
    },
    "network": {
        "interval": 5
    },
    "pulseaudio": {
        "scroll-step": 5
    },
    "backlight": {
        "device": "intel_backlight"
    },
    "battery": {
        "interval": 10
    }
}